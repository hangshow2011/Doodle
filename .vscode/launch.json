{
    "configurations": [
        {
            "name": "Maya_plug_debug",
            "type": "cppvsdbg",
            "request": "attach",
            "processId": "${command:pickProcess}"
        },
        {
            "name": "Python: 当前文件",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": [
                "--path=C:/Users/teXiao/Documents/maya/projects/default/scenes/test_uv_set_and_mat.ma",
                "--exportpath=C:/Users/teXiao/Documents/maya/projects/default/scenes"
            ]
        },
        {
            "name": "Doodle_debug",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${command:cmake.launchTargetPath}",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${command:cmake.launchTargetDirectory}",
            "environment": [
                {
                    "name": "PATH",
                    "value": "$PATH;"
                },
                {
                    "name": "squid",
                    "value": "clam"
                }
            ],
            "console": "integratedTerminal",
            "visualizerFile": "${workspaceFolder}/qt5.natvis"
        }
    ]
}